{
  "version": 2,
  "buildCommand": "npm run build:vercel",
  "outputDirectory": ".",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ],
  "functions": {
    "api/index.ts": {
      "maxDuration": 30,
      "memory": 1024
    }
  },
  "env": {
    "VERCEL": "1",
    "NODE_ENV": "production"
  }
}